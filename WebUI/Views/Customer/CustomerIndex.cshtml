@using Inv.WebUI.Resources
@{

    Layout = "~/Views/Shared/Layout.cshtml";
} 
<body class="materialdesign">
    <div class="content-inner-all"> 
        <div class="inbox-mailbox-area mg-b-15">
            <div class="container-fluid">
                <div class="row">
					<div class="col-lg-12">
						<div class="row">
							<div class="col-lg-2">
								<div class="inbox-email-menu-list compose-b-mg-30 shadow-reset">

									<ul class="nav nav-tabs">
										<li id="btnCustomers">
											<a data-toggle="tab" href="#Items"><span class="inbox-icon"><i class="fa fa-tasks" aria-hidden="true"></i></span>Customers</a>
										</li>


										<li>
											<label>Created</label>
										</li>
										<li>
											<input id="txtCreatedBy" name="CREATED_BY" type="text" disabled class="form-control" placeholder="Created By ">
											<input id="txtCreatedAt" name="CREATED_AT" type="text" disabled class="form-control" placeholder="Created At">
										</li>

										<li>
											<label>Updated</label>
										</li>
										<li>
											<input id="txtUpdatedBy" name="UPDATED_BY" type="text" disabled class="form-control" placeholder="Updated By ">
											<input id="txtUpdatedAt" name="UPDATED_AT" type="text" disabled class="form-control" placeholder="Updated At">
										</li>


									</ul>
								</div>
							</div>
							<div class="col-lg-10">
								<div id="Customers" class="tab-pane animated zoomInDown fade shadow-reset custom-inbox-message active in">
									<div class="admin-dashone-data-table-area mg-b-15">
										<div class="container-fluid">
											<div class="row">
												<div id="Grad1"></div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
                </div>
            </div>
        </div>
    </div>
</body>
<script src="~/ClientApp/Customer/CustomerCompany.js"></script>